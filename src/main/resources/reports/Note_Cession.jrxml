	<subDataset name="cessions_legales" uuid="56cbe35e-f6c9-4e10-9b9d-f5a694af8312">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="SynchronRE"/>
		<parameter name="aff_id" class="java.lang.Long"/>
		<queryString language="SQL">
			<![CDATA[SELECT
    distinct
    r.rep_id,
    r.aff_id,
    COALESCE(
            (SELECT pc.param_ces_leg_libelle
             FROM param_cession_legale pc
             WHERE pc.param_ces_leg_id = r.param_ces_legal_id),
            (SELECT t.name
             FROM type t
             WHERE t.type_id = r.typ_id)
        ) AS type_libelle,
    r.rep_capital,
   regexp_replace(to_char(r.rep_capital, 'FM999,999,999,999'), ',', ' ', 'g') AS rep_capital1,
    r.rep_taux,
    ROUND(r.rep_taux, 2) AS taux,
    r.typ_id,
    a.part_cedante,
    a.fac_smp_lci,
    SUM(r.rep_capital) OVER (PARTITION BY r.aff_id) AS somme_capital_affaire,
    vcltc.diffrence_montant - SUM(r.rep_capital) OVER (PARTITION BY r.aff_id) as diffrence_montant,
    regexp_replace(to_char(vcltc.diffrence_montant - SUM(r.rep_capital) OVER (PARTITION BY r.aff_id), 'FM999,999,999,999'), ',', ' ', 'g') AS diffrence_montant1,
    vcltc.diffrence_taux - SUM(r.rep_taux) OVER (PARTITION BY r.aff_id) as diffrence_taux,
    ROUND(vcltc.diffrence_taux - SUM(r.rep_taux) OVER (PARTITION BY r.aff_id) ,2) as diffrence_taux1
FROM affaire a
         JOIN repartition r ON r.aff_id = a.aff_id
         JOIN v_cession_legale_traite_calculee vcltc on a.aff_id = vcltc.aff_id
WHERE r.rep_id IN (
    SELECT r.rep_id
    FROM repartition r
             JOIN param_cession_legale pc ON r.param_ces_legal_id = pc.param_ces_leg_id
             JOIN type t ON pc.type_id = t.type_id
    WHERE r.aff_id = a.aff_id
      AND t.unique_code = 'PCL_SIMPLE'
      AND r.aff_id = $P{aff_id}
)
GROUP BY r.rep_id, r.aff_id, COALESCE(
            (SELECT pc.param_ces_leg_libelle
             FROM param_cession_legale pc
             WHERE pc.param_ces_leg_id = r.param_ces_legal_id),
            (SELECT t.name
             FROM type t
             WHERE t.type_id = r.typ_id)
        ), r.rep_capital, r.rep_taux, r.typ_id, a.part_cedante, a.fac_smp_lci, vcltc.diffrence_montant,vcltc.diffrence_taux]]>
		</queryString>
		<field name="rep_id" class="java.lang.Long"/>
		<field name="aff_id" class="java.lang.Long"/>
		<field name="type_libelle" class="java.lang.String"/>
		<field name="rep_capital" class="java.math.BigDecimal"/>
		<field name="rep_capital1" class="java.lang.String"/>
		<field name="rep_taux" class="java.math.BigDecimal"/>
		<field name="taux" class="java.math.BigDecimal"/>
		<field name="typ_id" class="java.lang.Long"/>
		<field name="part_cedante" class="java.math.BigDecimal"/>
		<field name="fac_smp_lci" class="java.math.BigDecimal"/>
		<field name="somme_capital_affaire" class="java.math.BigDecimal"/>
		<field name="diffrence_montant" class="java.math.BigDecimal"/>
		<field name="diffrence_montant1" class="java.lang.String"/>
		<field name="diffrence_taux" class="java.math.BigDecimal"/>
		<field name="diffrence_taux1" class="java.math.BigDecimal"/>
