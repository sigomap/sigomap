		<parameter name="aff_id" class="java.lang.Long"/>
		<queryString language="SQL">
			<![CDATA[SELECT
    distinct
    r.rep_id,
    r.aff_id,
    COALESCE(
            (SELECT pc.param_ces_leg_libelle
             FROM param_cession_legale pc
             WHERE pc.param_ces_leg_id = r.param_ces_legal_id),
            (SELECT t.name
             FROM type t
             WHERE t.type_id = r.typ_id)
        ) AS type_libelle,
    r.rep_capital,
     regexp_replace(to_char( r.rep_capital, 'FM999,999,999,999'), ',', ' ', 'g') AS rep_capital1,
    ROUND(r.rep_taux, 2) as taux,
    r.typ_id,
    a.part_cedante,
    a.fac_smp_lci,
    SUM(r.rep_capital) OVER (PARTITION BY r.aff_id) AS somme_capital_affaire,
     regexp_replace(to_char(a.part_cedante - SUM(r.rep_capital) OVER (PARTITION BY r.aff_id), 'FM999,999,999,999'), ',', ' ', 'g') AS diffrence_montant ,
  ROUND(
                    (a.part_cedante / a.fac_smp_lci) * 100 -
                    COALESCE(SUM(r.rep_taux) OVER (PARTITION BY r.aff_id), 0),
                    2
        ) as diffrence_taux

FROM affaire a
         JOIN repartition r ON r.aff_id = a.aff_id
WHERE r.rep_id IN (
    SELECT r.rep_id
    FROM repartition r
             JOIN param_cession_legale pc ON r.param_ces_legal_id = pc.param_ces_leg_id
             JOIN type t ON pc.type_id = t.type_id
    WHERE r.aff_id = a.aff_id
      AND t.unique_code = 'PCL_PF'
      AND r.aff_id = $P{aff_id}
    )
   OR r.rep_id IN (
SELECT r.rep_id
FROM repartition r
    JOIN type t ON r.typ_id = t.type_id
WHERE t.unique_code IN ('REP_CONSERVATION','REP_FACOB','REP_XL')
  AND r.aff_id = a.aff_id
  AND r.aff_id = $P{aff_id}
    )
GROUP BY r.rep_id, r.aff_id, type_libelle, r.rep_capital, r.rep_taux, r.typ_id, a.part_cedante, a.fac_smp_lci]]>
		</queryString>
