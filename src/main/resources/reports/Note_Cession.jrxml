	<subDataset name="placement" uuid="5daa503a-2815-4d1a-8382-493ed68e225a">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="SynchronRE"/>
		<property name="com.jaspersoft.studio.property.dataset.dialog.DatasetDialog.sash.w1" value="888"/>
		<property name="com.jaspersoft.studio.property.dataset.dialog.DatasetDialog.sash.w2" value="101"/>
		<parameter name="aff_id" class="java.lang.Long">
			<defaultValueExpression><![CDATA[$P{aff_id}]]></defaultValueExpression>
		</parameter>
		<parameter name="ces_id" class="java.lang.Long">
			<defaultValueExpression><![CDATA[$P{ces_id}]]></defaultValueExpression>
		</parameter>
		<queryString language="SQL">
			<![CDATA[SELECT repartition.aff_id,
       repartition.rep_capital,
       regexp_replace(to_char(repartition.rep_capital::numeric, 'FM999,999,999,999'), ',', ' ', 'g') AS rep_capital1 ,
       ROUND(repartition.rep_taux, 2) AS taux,
       repartition.rep_sous_commission,
       repartition.rep_taux_com_ced,
       ROUND(repartition.rep_taux_com_ced, 2) AS taux_com_ced,
       repartition.rep_taux_com_court,
       ROUND(repartition.rep_taux_com_court, 2) AS taux_com_court,
       repartition.typ_id,
       repartition.cessionnaire_id,
       repartition.rep_taux * affaire.fac_prime / 100::numeric AS prime_cedee,
       regexp_replace(to_char(repartition.rep_taux * affaire.fac_prime / 100::numeric::numeric, 'FM999,999,999,999'), ',', ' ', 'g') AS prime_cedee1 ,
       repartition.rep_taux_com_ced * (repartition.rep_taux * affaire.fac_prime / 100::numeric) / 100::numeric AS com_cedante,
       regexp_replace(to_char(repartition.rep_taux_com_ced * (repartition.rep_taux * affaire.fac_prime / 100::numeric) / 100::numeric::numeric, 'FM999,999,999,999'), ',', ' ', 'g') AS com_cedante1,
       repartition.rep_taux_com_court * (repartition.rep_taux * affaire.fac_prime / 100::numeric) / 100::numeric AS com_courtage,
       regexp_replace(to_char(repartition.rep_taux_com_court * (repartition.rep_taux * affaire.fac_prime / 100::numeric) / 100::numeric::numeric, 'FM999,999,999,999'), ',', ' ', 'g') AS com_courtage1 ,
       (repartition.rep_taux * affaire.fac_prime / 100::numeric) - (repartition.rep_taux_com_ced * (repartition.rep_taux * affaire.fac_prime / 100::numeric) / 100::numeric) - (repartition.rep_taux_com_court * (repartition.rep_taux * affaire.fac_prime / 100::numeric) / 100::numeric) AS prime_nette_cedee,
       regexp_replace(to_char((repartition.rep_taux * affaire.fac_prime / 100::numeric) - (repartition.rep_taux_com_ced * (repartition.rep_taux * affaire.fac_prime / 100::numeric) / 100::numeric) - (repartition.rep_taux_com_court * (repartition.rep_taux * affaire.fac_prime / 100::numeric) / 100::numeric)::numeric, 'FM999,999,999,999'), ',', ' ', 'g') AS prime_nette_cedee1 ,
       cessionnaire.ces_nom,
       affaire.aff_activite,
       affaire.aff_assure,
       affaire.aff_code,
       affaire.aff_date_echeance,
       affaire.aff_date_effet,
       affaire.fac_prime,
       affaire.fac_smp_lci,
       cedante.ced_nom_filiale,
       couverture.cou_libelle
FROM affaire,
     cedante,
     couverture,
     repartition,
     cessionnaire
WHERE repartition.cessionnaire_id = cessionnaire.ces_id
  AND repartition.aff_id = affaire.aff_id
  AND affaire.couverture_id = couverture.cou_id
  AND affaire.cedente_id = cedante.ced_id
  AND repartition.typ_id = 3
  and repartition.rep_statut=true
  AND repartition.aff_id = $P{aff_id}
  AND  repartition.cessionnaire_id =$P{ces_id}]]>
		</queryString>
